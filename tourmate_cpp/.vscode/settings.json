{
    "C_Cpp.default.cppStandard": "c++11",
    "C_Cpp.default.cStandard": "c17",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/src/headers"
    ],
    "C_Cpp.default.compilerPath": "g++",
    "C_Cpp.default.intelliSenseMode": "gcc-x64",
    "files.associations": {
        "*.h": "cpp",
        "*.cpp": "cpp",
        "iostream": "cpp",
        "fstream": "cpp",
        "string": "cpp",
        "vector": "cpp",
        "algorithm": "cpp",
        "sstream": "cpp",
        "iomanip": "cpp",
        "ctime": "cpp",
        "cstdlib": "cpp"
    },
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "editor.rulers": [100],
    "files.encoding": "utf8",
    "files.eol": "\n",
    "editor.formatOnSave": true,
    "C_Cpp.formatting": "vcFormat",
    "C_Cpp.vcFormat.indent.braces": false,
    "C_Cpp.vcFormat.indent.multiLineRelativeTo": "innermost",
    "C_Cpp.vcFormat.indent.preserveIndentationOfComments": true,
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "newLine",
    "C_Cpp.vcFormat.space.beforeFunctionOpenParenthesis": "remove",
    "workbench.colorCustomizations": {
        "activityBar.background": "#2f7c47",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#422c74",
        "activityBarBadge.foreground": "#e7e7e7",
        "statusBar.background": "#215732",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#2f7c47",
        "titleBar.activeBackground": "#215732",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#21573299",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "peacock.color": "#215732"
}